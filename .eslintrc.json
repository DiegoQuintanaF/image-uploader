{
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "warn"
  },
  "globals": {
    "document": "readonly",
    "window": "readonly",
    "location": "readonly",
    "FileReader": "readonly",
    "navigator": "readonly"
  }
}
